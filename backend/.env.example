# API Keys - Add the ones you want to use
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI Configuration
OPENAI_BASE_URL=https://api.openai.com/v1  # Change this for LiteLLM proxy or other OpenAI-compatible APIs

# JWT Secret
JWT_SECRET_KEY=your_jwt_secret_key_here

# Upload directory
UPLOAD_DIR=./uploads

# Server settings
HOST=0.0.0.0
PORT=8000

# CORS settings - set to false in production if serving from same domain
CORS_ENABLED=true
